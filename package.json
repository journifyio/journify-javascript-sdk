{
  "name": "@journifyio/js-sdk",
  "version": "0.0.199",
  "description": "Journify SDK in JavaScript",
  "main": "dist/lib/es5/index.js",
  "types": "dist/lib/es5/index.d.ts",
  "module": "dist/lib/es6/index.js",
  "sideEffects": false,
  "files": [
    "dist/"
  ],
  "repository": "git@github.com:journifyio/javascript-sdk.git",
  "homepage": "https://github.com/journifyio/javascript-sdk",
  "author": "Journifyio",
  "license": "MIT",
  "keywords": [
    "journify",
    "javascript",
    "sdk"
  ],
  "scripts": {
    "build": "npm-run-all setup clean update-version && tsc && tsc --target es6 --outDir dist/lib/es6 && webpack",
    "setup": "shx cp scripts/githooks/* .git/hooks/ && shx chmod +x .git/hooks/*",
    "clean": "shx rm -rf dist",
    "test": "jest",
    "lint": "eslint . --ext .ts --max-warnings=0",
    "update-version": "sh scripts/update-version-var.sh",
    "major-version": "npm --no-git-tag-version version major && npm run update-version",
    "minor-version": "npm --no-git-tag-version version minor && npm run update-version",
    "patch-version": "npm --no-git-tag-version version patch && npm run update-version",
    "build-watch": "chokidar \"src/lib/**/*.js\" \"src/lib/**/*.ts\" -i \"src/lib/generated/*.ts\" -i \"**/*.test.ts\" -c \"npm run build-watch-cmd\" --initial",
    "build-watch-cmd": "npm-run-all build copy-lib-to-playground && echo 'Build finished' && date",
    "copy-lib-to-playground": "mkdir -p examples/react-playground/node_modules/@journifyio/js-sdk && cp -r  dist/lib/es6/* examples/react-playground/node_modules/@journifyio/js-sdk",
    "playground": "npm run --prefix examples/react-playground start"
  },
  "dependencies": {
    "@sentry/browser": "8.33.0",
    "@types/js-cookie": "^3.0.6",
    "@types/lodash": "^4.14.202",
    "@types/node": "^18.6.1",
    "@types/uuid": "^9.0.8",
    "js-cookie": "^3.0.5",
    "liquidjs": "^10.10.0",
    "lodash": "^4.17.21",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.9",
    "@peculiar/webcrypto": "^1.4.6",
    "@sentry/webpack-plugin": "^2.16.1",
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "chokidar-cli": "^3.0.0",
    "eslint": "^8.21.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "2.8.3",
    "shx": "^0.3.4",
    "terser-webpack-plugin": "^5.3.10",
    "ts-jest": "^29.2.2",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "webpack": "5.102.1",
    "webpack-cli": "^5.1.4"
  }
}
